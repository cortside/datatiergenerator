<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="DataTierGenerator" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
    <xs:element name="DataTierGenerator">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element name="config">
                    <xs:complexType>
