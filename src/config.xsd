                                <xs:complexType>
                                    <xs:attribute name="name" type="xs:string" />
                                    <xs:attribute name="value" type="xs:string" />
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="entities">
                    <xs:complexType>
                        <xs:sequence>
