                                                                                        </xs:element>
                                                                                    </xs:sequence>
                                                                                    <xs:attribute name="name" type="xs:string" />
                                                                                    <xs:attribute name="unique" type="BooleanEnum" />
                                                                                    <xs:attribute name="clustered" type="BooleanEnum" />
                                                                                </xs:complexType>
                                                                            </xs:element>
                                                                        </xs:sequence>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                            </xs:sequence>
                                                            <xs:attribute name="name" type="xs:string" />
                                                            <xs:attribute name="view" type="xs:string" />
