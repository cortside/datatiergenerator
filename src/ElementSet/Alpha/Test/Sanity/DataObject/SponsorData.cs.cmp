using System;

using Spring2.Core.Types;

namespace Golf.Tournament.DataObject {
    public class SponsorData : Spring2.Core.DataObject.DataObject {

	private StringType name = StringType.DEFAULT;
	private StringType logo = StringType.DEFAULT;

	public static readonly String NAME = "Name";
	public static readonly String LOGO = "Logo";

	public StringType Name {
	    get { return this.name; }
	    set { this.name = value; }
	}

	public StringType Logo {
	    get { return this.logo; }
	    set { this.logo = value; }
	}
    }
}
