using System;

using Spring2.Core.Types;

namespace Golf.Tournament.DataObject {
    public class ALaCarteItemData : Spring2.Core.DataObject.DataObject {

	private StringType description = StringType.DEFAULT;
	private CurrencyType price = CurrencyType.DEFAULT;

	public static readonly String DESCRIPTION = "Description";
	public static readonly String PRICE = "Price";

	public StringType Description {
	    get { return this.description; }
	    set { this.description = value; }
	}

	public CurrencyType Price {
	    get { return this.price; }
	    set { this.price = value; }
	}
    }
}
