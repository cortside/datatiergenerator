using System;

using Spring2.Core.Types;

namespace Golf.Tournament.DataObject {
    public class TournamentFeeData : Spring2.Core.DataObject.DataObject {

	private IdType tournamentFeeId = IdType.DEFAULT;
	private IdType tournamentId = IdType.DEFAULT;
	private StringType key = StringType.DEFAULT;
	private CurrencyType fee = CurrencyType.DEFAULT;

	public static readonly String TOURNAMENTFEEID = "TournamentFeeId";
	public static readonly String TOURNAMENTID = "TournamentId";
	public static readonly String KEY = "Key";
	public static readonly String FEE = "Fee";

	public IdType TournamentFeeId {
	    get { return this.tournamentFeeId; }
	    set { this.tournamentFeeId = value; }
	}

	public IdType TournamentId {
	    get { return this.tournamentId; }
	    set { this.tournamentId = value; }
	}

	public StringType Key {
	    get { return this.key; }
	    set { this.key = value; }
	}

	public CurrencyType Fee {
	    get { return this.fee; }
	    set { this.fee = value; }
	}
    }
}
